# 1) Валідні дужки
def isValid(s: str) -> bool:
    pairs = {')': '(', ']': '[', '}': '{'}
    st = []
    for ch in s:
        if ch in '([{':
            st.append(ch)
        else:
            if not st or st[-1] != pairs[ch]:
                return False
            st.pop()
    return not st
# Складність: O(n) часу, O(n) пам’яті (стек у гіршому випадку).
