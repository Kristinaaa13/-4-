# 3) Мінімальний стек (усі операції O(1))
class MinStack:
    def __init__(self):
        self.st = []  # зберігаємо (val, current_min)

    def push(self, val: int) -> None:
        cur_min = val if not self.st else min(val, self.st[-1][1])
        self.st.append((val, cur_min))

    def pop(self) -> None:
        self.st.pop()

    def top(self) -> int:
        return self.st[-1][0]

    def getMin(self) -> int:
        return self.st[-1][1]
# Складність: усі операції O(1), пам’ять O(n).
